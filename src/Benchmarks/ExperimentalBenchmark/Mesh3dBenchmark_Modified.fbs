
namespace BenchmarkCore.Modified;

struct Vector4 (fs_writeThrough)
{
    x:float;
    y:float;
    z:float;
    w:float;
}

struct Vector3Int (fs_writeThrough)
{
    x:int32;
    y:int32;
    z:int32;
}

struct Vector3  (fs_writeThrough)
{
    x:float;
    y:float;
    z:float;
}

struct Vector2Int (fs_writeThrough)
{
    x:int32;
    y:int32;
}

struct Vector2 (fs_writeThrough)
{
    x:float;
    y:float;
}

struct Color (fs_writeThrough)
{
    r: ubyte;
    g: ubyte;
    b: ubyte;
    a: ubyte;
}

struct Voxel (fs_writeThrough)
{
    VoxelType: ubyte;
    SubType : ubyte;
    Hp:ubyte;
    Unused:ubyte;
}

struct MutableUInt32 (fs_writeThrough)
{
    Value : uint32;
}

struct MutableUShort (fs_writeThrough)
{
    Value : ushort;
}

table Mesh (fs_serializer:Lazy)
{
    vertices:   [Vector3];
    normals:    [Vector3];
    uv:         [Vector2];
    color:      [Color];
    triangles:  [MutableUShort];
    filledLength: MutableUInt32;
}

table VoxelRegion3D (fs_serializer:Lazy)
{
    location: Vector3Int;
    iteration: MutableUInt32;
    size: ushort;
    voxels: [Voxel];
} 